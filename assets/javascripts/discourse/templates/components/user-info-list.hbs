<ul class="user-info-list">
  {{#each users as |user|}}
    <li class="user-info-item">
      <UserInfo @user={{user}}>
        <div>{{cakeday-date user.cakedate isBirthday=isBirthday}}</div>
      </UserInfo>
    </li>
  {{else}}
    <div class="user-info-empty-message"><h4>{{yield}}</h4></div>
  {{/each}}
</ul>